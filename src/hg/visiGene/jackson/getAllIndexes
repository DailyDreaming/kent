#!/bin/tcsh
set table = "$1"
set sql = "select name from sysobjects where type='U' order by name"
query "$sql" "bcp" | sed -e 's/|//g' > temptablelist

#cat temptablelist

set tables = ( `cat temptablelist` )

set skip = 1
while ("$tables" != "")
    set table = $tables[1] 
    shift tables
    # I already loaded some - don't need to do it again.
    if ("$table" == "PRB_Notes") then
	set skip = 0
    endif
    if ($skip == 1) then
	echo "skipping $table"
    else	    
	echo "indexing $table"
    	getindex $table
    endif
end

	
