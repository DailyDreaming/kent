#!/bin/tcsh
set sql = "select _Image_key from IMG_Image where _Image_key > 0" 
query "$sql" "bcp" | sed -e 's/|//g' > _Image_key.list
#cat _Image_key.list

# start with a new list
rm -f image-pix-map.tab
rm -f images.problems

set images = ( `cat _Image_key.list` )
#echo $images

while ( $#images != 0 )
    set image = $images[1] 
    shift images
    echo $image
    # I already loaded some images, don't need to do it again.
    #if ("$image" != "1705") then
     	getimage $image
    #endif
end

	
