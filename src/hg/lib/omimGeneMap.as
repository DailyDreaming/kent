table omimGeneMap
"This table contains the same data as the genemap file downloaded from OMIM"
    (
    string numbering;	"Numbering system, in the format  Chromosome.Map_Entry_Number"
    int month;		"Month entered"
    int day;		"Day entered"
    int year;		"Year entered"
    string location;	"Location"
    string geneSymbol;	"Gene Symbol(s)"
    char geneStatus;	"Gene Status"
    string title1;	"Title line1"
    string title2;	"Title line 2"
    int omimId;		"MIM Number"
    string method;	"Method"
    string comment1;	"Comments line 1"
    string comment2;	"Comments line 2"
    string disorders1;	"Disorders line 1"
    string disorders2;	"Disorders line 2"
    string disorders3;	"Disorders line 3"
    string mouseCorrelate;	"Mouse correlate"
    string reference;		"Reference"
    )


