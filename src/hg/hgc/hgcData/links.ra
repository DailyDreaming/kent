#this keeps information for links for the hgc cgi

name ORegAnno
url http://www.oreganno.org/oregano/recordview.action?recid=%s
acc yes

name HbVar
url http://globin.bx.psu.edu/cgi-bin/hbvar/query_vars3?mode=output&display_format=page&id=%s
acc yes

name GenPhen
url http://globin.bx.psu.edu/cgi-bin/genphen/test_gpquery?display_format=page&mode=Submit+Query&alleleId=%s
acc yes

name UniProtVar
url http://us.expasy.org/cgi-bin/get-sprot-variant.pl?%s
acc yes

name UniProtSearch
url http://us.expasy.org/cgi-bin/sprot-search-ful?SEARCH=%s
acc yes

name OMIM
url http://www.ncbi.nlm.nih.gov/entrez/dispomim.cgi?id=%s
acc yes

name PAHdb
url http://www.pahdb.mcgill.ca/cgi-bin/pahdb/pahdbsearch.cgi?Field=id_mut&OrderedField=nucl_no&Value=%s&SortType=Asc&Search=Mutation&Go=1&ToShowFrom=0&F2S1=id_mut&F2S2=nucl_no&F2S3=syst_name&F2S5=amino_acid&F2S6=mut_name&F2S7=other_name&F2S17=Mutation.comment
acc yes

name BGMUT
url http://www.ncbi.nlm.nih.gov/projects/mhc/xslcgi.fcgi?cmd=bgmut/allele_details&id=%s
acc yes

name CFMDB
url http://www3.genet.sickkids.on.ca/cftr/MutationDetailPage.external?sp=%s
acc yes

name dbSNP
url http://www.ncbi.nih.gov/SNP/snp_ref.cgi?type=rs&rs=%s
acc yes

name uniProtDis
label UniProt disease
dataSql select commentVal.val from uniProt.comment, uniProt.commentVal where comment.acc='%s' and commentType=21 and comment.commentVal=commentVal.id
dataId spId

name uniProtPoly
label UniProt polymorphism
dataSql select commentVal.val from uniProt.comment, uniProt.commentVal where comment.acc='%s' and commentType=16 and comment.commentVal=commentVal.id
dataId spId

name MPlink
label Mammalian Phenotype (MP)
url http://www.informatics.jax.org/searches/Phat.cgi?id=%s

name omimTitle2
label OMIM title
dataSql select title, omimId from omimTitle where omimId = '%s'
dataId mrna
dataLink OMIM
dataLinkCol 2

