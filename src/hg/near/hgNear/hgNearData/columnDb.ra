#This file contains definitions of columns common to all genomes.
#
name debug
shortLabel debug
longLabel debugging column on hgwdev
priority 0.5
visibility off
type debug

name num
shortLabel #
longLabel item number in displayed list/select gene
priority 1
visibility on
type num

name name
shortLabel Name
longLabel gene name/select gene
priority 2
visibility on
type knownName kgXref kgID geneSymbol
search fuzzy
searchLabel Known Gene Names

name proteinName
shortLabel SwissProt
longLabel SwissProt protein display ID
priority 2.1
visibility off
type lookup kgXref kgID spDisplayID
search fuzzy
itemUrl http://us.expasy.org/cgi-bin/niceprot.pl?%s

name refSeq
shortLabel RefSeq
longLabel NCBI RefSeq Gene Accession
priority 2.2
visibility off
type lookup knownToRefSeq name value
itemUrl http://www.ncbi.nlm.nih.gov/LocusLink/list.cgi?V=0&Q=%s

name acc
shortLabel GenBank
longLabel GenBank mRNA accession
priority 2.3
visibility off
type acc
itemUrl http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=Nucleotide&term=%s&doptcmdl=GenBank&tool=genome.ucsc.edu

name ensembl
shortLabel Ensembl
longLabel Ensembl transcript ID
priority 2.5
visibility off
type lookup knownToEnsembl name value
search prefix

name bitScore
shortLabel Bits
longLabel NCBI BLASTP bit score
priority 4
visibility off
type distance knownBlastTab query target bitScore

name eVal
shortLabel E Value
longLabel NCBI BLASTP E value
priority 5
visibility on
type distance knownBlastTab query target eValue

name percentId
shortLabel %ID
longLabel NCBI BLASTP percent identity
priority 6
visibility off
type distance knownBlastTab query target identity

name knownPos
shortLabel Genome Position
longLabel Genome position/link to Genome Browser
priority 7
visibility on
type knownPos 

name codingSnps
shortLabel Coding SNPs
longLabel Simple nucleotide polymorphisms in coding regions
priority 7.5
visibility off
type association knownToCdsSnp
queryFull select name,value from knownToCdsSnp
queryOne select value,value from knownToCdsSnp where name = '%s'
invQueryOne select name from knownToCdsSnp where value = '%s'
itemUrl http://www.ncbi.nlm.nih.gov/SNP/snp_ref.cgi?type=rs&rs=%s

name humanOrthologs
shortLabel Human
longLabel Human ortholog (best blastp hit to UCSC Known hg16)
priority 7.9
visibility off
type lookup hgBlastTab query target
itemUrl http://genome.ucsc.edu/cgi-bin/hgTracks?org=human&position=%s

name mouseOrthologs
shortLabel Mouse
longLabel Mouse ortholog (best blastp hit to UCSC Known mm3)
priority 8.0
visibility off
type lookup mmBlastTab query target
itemUrl http://genome.ucsc.edu/cgi-bin/hgTracks?org=mouse&position=%s

name zebrafishOrthologs
shortLabel Zebrafish
longLabel Danio rario ortholog (best blastp hit to Ensembl)
priority 8.1
visibility off
type lookup drBlastTab query target
itemUrl http://www.ensembl.org/Danio_rerio/protview?peptide=%s

name flyOrthologs
shortLabel Drosophila
longLabel D. melanogaster ortholog (best blastp hit to flybase proteins)
priority 8.3
visibility off
type lookup dmBlastTab query target
itemUrl http://flybase.bio.indiana.edu/.bin/fbidq.html?%s

name wormOrthologs
shortLabel C. elegans
longLabel C. elegans ortholog (best blastp hit to wormpep)
priority 8.4
visibility off
type lookup ceBlastTab query target
itemUrl http://www.wormbase.org/db/gene/gene?name=%s

name yeastOrthologs
shortLabel Yeast
longLabel Saccharomyces cerevisiae ortholog (best blastp hit to RefSeq)
priority 8.5
visibility off
type lookup scBlastTab query target
itemUrl http://db.yeastgenome.org/cgi-bin/SGD/locus.pl?locus=%s

name pfamDomains
shortLabel Pfam Domains
longLabel Protein family domain structure.
priority 9
visibility off
type pfam proteins072003.pfamXref proteins072003.pfamDesc
queryFull select swissDisplayID,description from proteins072003.pfamXref,proteins072003.pfamDesc where pfamXref.pfamAC = pfamDesc.pfamAC
queryOne select description,proteins072003.pfamDesc.pfamAC from proteins072003.pfamXref,proteins072003.pfamDesc where swissDisplayID = '%s' and pfamXref.pfamAC = pfamDesc.pfamAC order by description
invQueryOne select swissDisplayID from proteins072003.pfamXref,proteins072003.pfamDesc where description = '%s' and pfamXref.pfamAC = pfamDesc.pfamAC
protKey on
itemUrl http://www.sanger.ac.uk/cgi-bin/Pfam/getacc?%s
protDb proteins072003

name go
shortLabel Gene Ontology
longLabel Gene Ontology (GO) terms associated with gene.
priority 10
visibility off
type go
colWidth 50

name description
shortLabel Description
longLabel Short description line/link to details page
priority 20
visibility on
type knownDetails kgXref kgID description
search fuzzy
noKeys on
searchLabel Known Gene Descriptions
colWidth 50

