# This contains C. elegans specific column information.
#
name name
type lookup sgdToName name value
selfLink on

name acc
shortLabel SGD ORF
longLabel SGD ORF ID / Link to SGD
visibility on
itemUrl http://db.yeastgenome.org/cgi-bin/SGD/locus.pl?locus=%s

name proteinName
type lookup sgdToSwissProt name value
search prefix
itemUrl http://us.expasy.org/cgi-bin/niceprot.pl?%s

#name refSeq
#type lookup sgdToRefSeq name value

#name locusLink
#type lookup sgdToLocusLink name value

name bitScore
type distance sgdBlastTab query target bitScore

name eVal
type distance sgdBlastTab query target eValue

name percentId
type distance sgdBlastTab query target identity

name pdb
queryFull select acc,pdb from  proteins092903.pdbSP,swissProt.displayId where val=sp 
queryOne select pdb,pdb from proteins092903.pdbSP,swissProt.displayId where val=sp and acc='%s'
invQueryOne select acc from proteins092903.pdbSP,swissProt.displayId where val=sp and pdb='%s'

name pfamDomains
queryFull select swissAC,description from proteins092903.pfamXref,proteins092903.pfamDesc where pfamXref.pfamAC = pfamDesc.pfamAC
queryOne select description,proteins092903.pfamDesc.pfamAC from proteins092903.pfamXref,proteins092903.pfamDesc where swissAC = '%s' and pfamXref.pfamAC = pfamDesc.pfamAC order by description
invQueryOne select swissAC from proteins092903.pfamXref,proteins092903.pfamDesc where description = '%s' and pfamXref.pfamAC = pfamDesc.pfamAC

name go
goaIdColumn dbObjectId

name description
type knownDetails sgdDescription name description

