# This contains D. melanogaster specific column information.
#
name name
type lookup bdgpSwissProt bdgpName symbol
selfLink on

name acc
shortLabel BDGP
longLabel BDGP Gene ID
visibility on
itemUrl http://www.fruitfly.org/cgi-bin/annot/gene?%s

name proteinName
type lookup bdgpGene name proteinID
search fuzzy
itemUrl http://us.expasy.org/cgi-bin/niceprot.pl?%s

name refSeq
type lookup bdgpToRefSeq name value

name altSplice
shortLabel Alt Splice
longLabel Alternate-Splicing Curated by Zahler Lab
priority 2.5
visibility off
type lookup altSplice name value

name arbLifeCycleMedian
shortLabel Median Expression
longLabel Median Arbeitman et al. 2002 Life-cycle Expression Data
priority 2.7
visibility on
type expRatio bdgpToArb hgFixed.arbFlyLifeMedianRatio hgFixed.arbFlyLifeMedianExps
max 3.0
representatives 0,1,2,3,4,5,6

name arbLifeCycleFull
shortLabel Long Expression
longLabel All Arbeitman et al. 2002 Life-cycle Data Expression Data
priority 2.8
visibility off
type expRatio bdgpToArb hgFixed.arbFlyLifeAllRatio hgFixed.arbFlyLifeAllExps
max 3.0
representatives 8,11,15,16,-1,10,12,17,-1,13,18,21,22,-1,23,4,20,-1,6,5,14,-1,7,9,19,-1,0,1,2,3,

name bitScore
type distance bdgpBlastTab query target bitScore

name eVal
type distance bdgpBlastTab query target eValue

name percentId
type distance bdgpBlastTab query target identity

name description
type lookup bdgpSwissProt bdgpName spGeneName

