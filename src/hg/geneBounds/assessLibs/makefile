include ../../../inc/common.mk

L = -lm $(MYSQLLIBS)
MYLIBDIR = ../../../lib/$(MACHTYPE)
MYLIBS =  $(MYLIBDIR)/jkhgap.a $(MYLIBDIR)/jkweb.a

O = assessLibs.o 

assessLibs: $O $(MYLIBS)
	${CC} ${COPT} ${CFLAGS} -o $(HOME)/bin/$(MACHTYPE)/assessLibs $O $(MYLIBS) $L
	${STRIP} $(HOME)/bin/$(MACHTYPE)/assessLibs${EXE}
