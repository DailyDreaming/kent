include ../../../inc/common.mk

L = -lm
MYLIBDIR = ../../../lib/$(MACHTYPE)
MYLIBS =  $(MYLIBDIR)/jkweb.a

O = axtToMaf.o

axtToMaf: $O $(MYLIBS)
	${CC} ${COPT} ${CFLAGS} -o $(HOME)/bin/$(MACHTYPE)/axtToMaf $O $(MYLIBS) $L
	${STRIP} $(HOME)/bin/$(MACHTYPE)/axtToMaf${EXE}

test: $(INSTALLDIR)/$(PROG)
	cd tests && ${MAKE} test
