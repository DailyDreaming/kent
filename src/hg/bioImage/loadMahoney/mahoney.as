
table mahoney
"Tabular data from Paul Gray/Mahoney spread sheet"
    (
    string empty;	"Always blank"
    int mtf;	"mahoney id"
    string sameAs;	"comma sep list of ids"
    string geneName;	"HUGO name if available"
    string domain;	"Domain that marks it as a transcription factor"
    string genbank;	"genbank (including refSeq) ID"
    string locusId;	"locus link id"
    string unigene;	"unigene id"
    string fragSize;	"PCR product size"
    string linzyme;	"Restriction enzyme to linearize"
    string rnaPol;	"DNA->RNA enzyme, usually T7"
    string cloneVector;	"Cloning vector"
    string seqVerified;	"X if true"
    string startLoc;	"start base pair location"
    string endLoc;	"end base pair location"
    string fPrimer;	"Forward primer"
    string rPrimer;	"Reverse primer"
    string fAdaptor;	"Start adapter enzyme"
    string rAdaptor;	"End adapter enzyme"
    )
