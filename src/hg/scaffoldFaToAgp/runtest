#!/bin/csh -f

# run on test file, diff with "golden" file

${HOME}/bin/i386/scaffoldFaToAgp test.fa  >& /dev/null

diff test.agp test.agp.gold > /dev/null
set status1 = $status
diff test.lft test.lft.gold > /dev/null
if ($status || $status1) then
    echo '*** FAIL ***'
endif
rm -f test.lft test.agp
