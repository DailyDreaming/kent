include ../../../inc/common.mk

L = ${MYSQLLIBS} -lm
MYLIBDIR = ../../../lib/${MACHTYPE}
MYLIBS =  ${MYLIBDIR}/jkhgap.a ${MYLIBDIR}/jkweb.a

O = fakeSeqDiffs.o

fakeSeqDiffs: $O ${MYLIBS}
	${CC} ${COPT} -o ${HOME}/bin/${MACHTYPE}/fakeSeqDiffs $O ${MYLIBS} $L
	${STRIP} ${HOME}/bin/${MACHTYPE}/fakeSeqDiffs${EXE}

clean:
	rm -f $O
