# Fly-specific link info.
# This contains info to construct the quick links. 

name genome
tables bdgpGene 
idSql select chrom,txStart+1,txEnd from bdgpGene where name = '%s'

name family
tables bdgpGene
idSql select name from bdgpGene where name = '%s'

name bdgp
shortLabel BDGP
tables bdgpGene
idSql select name from bdgpGene where name = '%s'
url http://www.fruitfly.org/cgi-bin/annot/gene?%s
priority 26

name swissProt
tables bdgpGene
idSql select proteinID from bdgpGene where name = '%s'

name refSeq
shortLabel RefSeq
tables bdgpToRefSeq
idSql select value from bdgpToRefSeq where name = '%s'
url http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=Nucleotide&term=%s&doptcmdl=GenBank&tool=genome.ucsc.edu
priority 40

