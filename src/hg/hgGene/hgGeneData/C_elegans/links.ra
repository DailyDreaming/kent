name genome
tables sangerGene 
idSql select chrom,txStart+1,txEnd from sangerGene where name = '%s'

name family
tables sangerGene
idSql select name from sangerGene where name = '%s'

name wormBase
shortLabel WormBase
tables sangerGene
idSql select name from sangerGene where name = '%s'
