# This file contains info to process the homologous genes
# in other species links.

name human
shortLabel Human
tables hgBlastTab 
idSql select target from hgBlastTab where query = '%s'
pepTable knownGenePep
genomeUrl ../cgi-bin/hgTracks?org=Human&position=%s
sorterUrl ../cgi-bin/hgNear?org=Human&near.oldOrg=Human&db=0&near_search=%s
otherUrl http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Search&db=Nucleotide&term=%s&doptcmdl=GenBank&tool=genome.ucsc.edu
otherName GenBank
priority 10

name mouse
shortLabel Mouse
tables mmBlastTab 
idSql select target from mmBlastTab where query = '%s'
pepTable knownGenePep
genomeUrl ../cgi-bin/hgTracks?org=Mouse&position=%s
sorterUrl ../cgi-bin/hgNear?org=Mouse&near.oldOrg=Mouse&db=0&near_search=%s
otherName Jackson Labs
otherUrl http://www.informatics.jax.org/searches/accession_report.cgi?id=%s
otherIdSql select swissProt.extDbRef.extAcc1 from swissProt.extDbRef,swissProt.extDb,kgXref where swissProt.extDb.val = 'MGD' and swissProt.extDb.id = swissProt.extDbRef.extDb and swissProt.extDbRef.acc = kgXref.spID and kgXref.kgID = '%s' 
priority 20

