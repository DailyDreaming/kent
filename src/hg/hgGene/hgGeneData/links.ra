name locusLink
shortLabel LocusLink
tables kgXref refLink
idSql select refLink.locusLinkId from kgXref,refLink where kgID = '%s' and kgXref.refseq = refLink.mrnaAcc
url http://www.ncbi.nlm.nih.gov/LocusLink/LocRpt.cgi?l=%s
priority 45

