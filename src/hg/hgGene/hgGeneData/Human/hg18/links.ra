# hg18 link info.

name genome
shortLabel Genome Browser
tables knownGene
idSql select chrom,txStart+1,txEnd from knownGene where name = '%s'
url ../cgi-bin/hgTracks?position=%s:%s-%s&knownGene=pack
nameFormat %s:%s-%s
hgsid on
dbInUrl on
priority 10

name protBrowser
shortLabel Proteome Browser
tables kgXref
idSql select spID from kgXref where kgID = '%s'
nameSql select spId from kgXref where kgID = '%s'
url ../cgi-bin/pbTracks?proteinID=%s
hgsid on
dbInUrl on
priority 21

name exonPrimer
shortLabel ExonPrimer
tables kgXref
idSql select kgID from kgXref where kgID = '%s'
url http://ihg.gsf.de/cgi-bin/primer/ExonPrimerUCSC.pl?db=hg17&acc=%s
priority 95

