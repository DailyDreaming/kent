# Proteome Browser link info.

name genome
shortLabel Genome Browser
tables knownGene
idSql select chrom,txStart+1,txEnd from knownGene where name = '%s'
url ../cgi-bin/hgTracks?position=%s:%s-%s&knownGene=pack
nameFormat %s:%s-%s
hgsid on
dbInUrl on
priority 10

name exonPrimer
shortLabel ExonPrimer
tables kgXref
idSql select kgID from kgXref where kgID = '%s'
url http://ihg.gsf.de/cgi-bin/primer/ExonPrimerUCSC.pl?db=mm6&acc=%s
priority 95
