# Mouse-specific link info.

name jax
shortLabel Jackson Labs
tables kgXref
idSql select swissProt.extDbRef.extAcc1 from swissProt.extDbRef,swissProt.extDb,kgXref where swissProt.extDb.val = 'MGD' and swissProt.extDb.id = swissProt.extDbRef.extDb and swissProt.extDbRef.acc = kgXref.spID and kgXref.kgID = '%s' 
priority 25
url http://www.informatics.jax.org/searches/accession_report.cgi?id='%s'

name geneLynx
shortLabel GeneLynx
tables kgXref
idSql select geneSymbol from kgXref where kgID = '%s'
url http://www.genelynx.org/cgi-bin/linklist?tableitem=GLID_NAME.name&IDlist=%s&dir=1
priority 70

name geneCards
shortLabel GeneCards
tables kgXref
idSql select geneSymbol from kgXref where kgID = '%s'
url http://bioinfo.weizmann.ac.il/cards-bin/cardsearch.pl?search=%s
priority 80

name cgap
url http://cgap.nci.nih.gov/Genes/GeneInfo?ORG=Mm&CID=%s

name ensembl
url http://www.ensembl.org/Mus_musculus/geneview?transcript=%s

