include ../../inc/common.mk

L = ${MYSQLLIBS} -lm 
MYLIBDIR = ../../lib/${MACHTYPE}
MYLIBS =  ${MYLIBDIR}/jkhgap.a ${MYLIBDIR}/jkweb.a

A = pslPairs

O = $A.o

compile: $O 
	${CC} ${COPT} ${CFLAGS} $O ${MYLIBS} ${L} 
	mv a.out $A
	${STRIP} $A${EXE}
	chmod a+rx $A
	cp $A ${HOME}/bin/${MACHTYPE}

debug: $O
	${CC} ${COPT} ${CFLAGS} $O ${MYLIBS} ${L} 
	mv a.out $A

lib:
	cd ../../lib; make

tags:  
	ctags *.c ../lib/*.c ../inc/*.h ../../lib/*.c ../../inc/*.h

clean::
	rm -f pslPairs ${O}
