MAKE GCPERCENT (DONE 2003-05-12 - Hiram)
     ssh hgwdev
     mkdir -p /cluster/store4/worm/ce1/bed/gcPercent
     cd /cluster/store4/worm/ce1/bed/gcPercent
     hgsql ce1  < ~/kent/src/hg/lib/gcPercent.sql
