    faSplit sequence /iscratch/i/mrna.134/Caenorhabditis_elegans/mrna.fa \
		1000000 mRNA/m
    mkdir EST
    faSplit sequence /iscratch/i/mrna.134/Caenorhabditis_elegans/est.fa \
		1000000 EST/e
    ~kent/bin/iSync

    ssh kkstore
    cd ~/ce1/bed
