#  GC 20K BASE WIGGLE TRACK (DONE - 2004-01-28 - Hiram)
    #    working on kksilo, the fileserver for this data.
    #    The grep in the pipeline below is "\tGC\t" but I
    #    can't figure out what escaped character to use to represent
    #    that.  Also, the hgGcPercent was in my home bin since I was
    #    working on it for this process.  It will be checked in when it
    #    works OK.  It is still being tweaked
    ssh kksilo
    mkdir /cluster/data/panTro1/bed/gc5Base
    cd /cluster/data/panTro1/bed/gc5Base
    mkdir asciiData wigData dataLimits
for n in ../../nib/*.nib
do
    c=`basename ${n} | sed -e "s/.nib//"`
    C=`echo $c | sed -e "s/chr//"`
    echo "working on ${c} - ${C}"
    $HOME/bin/i386/hgGcPercent -chr=${c} -doGaps \
	-file=stdout -win=20000 hg16 ../../nib | grep "	GC	" | \
    awk '
{
    bases = $3 - $2
    perCent = $5/10.0
    printf "%d\t%.1f\n", $2+1, perCent
}' | wigAsciiToBinary -dataSpan=20000 -chrom=${c} \
	-wibFile=wigData/gc20KBase_${C} -name=${C} stdin > dataLimits/${c}
echo "done ${c}"
done

