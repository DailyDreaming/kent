#!/usr/bin/perl -w
use strict;

my @clonePrefixes = ("CH211-", "ch211-", "DKEY-", "DKEYP-", "RP71-", "BUSM1-", "CH73-", "CHORI-");
my %cloneHash = qw {
   CH211-  zC
   DKEY-   zK
   DKEYP-  zKp
   RP71-   bZ
   BUSM1-  dZ
   CH73-   CHORI73_
};

while (<STDIN>) 
{
my ($l, $c, $intPref);
$l = $_;
foreach $c (@clonePrefixes)
   {
