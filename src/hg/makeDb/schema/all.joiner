# This contains linkage information between various fields
# in various databases.
#
# So far have looked through hg13,hg15,hg16,mm4,mm3,rn3,ce1,dm1,sacCer1,
# panTro1,galGal2,fr1,cb1,hgFixed,go, and swissProt databases

# Define databases used for various organisms
set hg hg13,hg15,hg16
set mm mm3,mm4
set rn rn2,rn3
set fr fr1
set ce ce1
set cb cb1
set dm dm1
set dp dp1
set sacCer sacCer1
set panTro panTro1
set galGal galGal2

# Define all organism/assembly specific databases.
set gbd $hg,$mm,$rn,$fr,$ce,$cb,$dm,$dp,$sacCer,$panTro,$galGal

# Only consider one of members of gbd at a time.
exclusiveSet $gbd

# Define other databases that we check
set otherDb swissProt,go,hgFixed

# Define protein databases (currently not checked)
set proteinDb proteins,proteins0111,proteins0305,proteins031112,proteins040115,proteins040115,proteins0405,proteins070403,proteins072003,proteins092903,proteins1129

# Define zoo databases (currently not checked)
set zooDb zooBaboon3,zooCat3,zooChicken3,zooChimp3,zooCow3,zooDog3,zooFugu3,zooHuman3,zooMouse3,zooPig3,zooRat3,zooTetra3,zooZebrafish3


# Set up list of databases we ignore and those we check.  Program
# will complain about other databases.
databasesChecked $gbd,$otherDb
databasesIgnored mysql,lost+found,$proteinDb,$zooDb,sc1,hgcentraltest,hgcentralbeta

# Define various non-self chains
set chainDest Hg15,Hg16,Pt0,Mm2,Mm3,Mm4,Rn2,Rn3,Cb1,Dp1,PanTro1,GalGal2

# Define databases that support known genes
set kgDb $hg,$mm,$rn

# Define databases that support family browser
set familyDb $hg,$mm,$ce,$sacCer,$dm

# Magic for tables split between chromosomes
set split splitPrefix=chr%_

# Define the shared identifiers and the fields they appear in.


# GO - Gene Ontology Info

identifier goAccession external=GO
"Gene ontology term accessions.  Example - GO:0003574"
     go.term.acc 
     go.goaPart.goId minCheck=0.999

identifier goAccessionChopped
"Gene ontology term accessions without the GO:"
     go.term.acc chopBefore=GO:
     $dm.bdgpGeneInfo.go comma minCheck=0.99
     $dm.bdgpNonCodingInfo.go comma minCheck=0.99

identifier goId external=GO
"Gene ontology term ID's.  Shouldn't be used outside of go database"
     go.term.id 
     go.term2term.term1_id
     go.term2term.term2_id
     go.term_audit.term_id
     go.term_dbxref.term_id
     go.term_definition.term_id unique 
     go.term_synonym.term_id 
     go.graph_path.term1_id
     go.graph_path.term2_id

identifier goDbXrefId external=GO
"Gene ontology database cross-reference"
     go.dbxref.id
     go.term_dbxref.dbxref_id

# SwissProt Related Stuff

identifier swissProtId external=SwissProt
"Swiss Prot ID. Same as accession for uncurated trEMBL proteins"
     swissProt.displayId.val 
     go.goaPart.dbObjectSymbol minCheck=0.95
     $gbd.dupSpMrna.proteinID minCheck=0.80
     $gbd.dupSpMrna.dupProteinID minCheck=0.50
     $kgDb.kgProtAlias.displayID minCheck=0.80
     $kgDb.kgXref.spDisplayID minCheck=0.80
     $kgDb.knownCanonical.protein minCheck=0.80
     $kgDb.knownGene.proteinID minCheck=0.80
     $gbd.spMrna.spID minCheck=0.80
     $dm.bdgpSwissProt.spSymbol minCheck=0.80
     $dm.flyBaseSwissProt.spSymbol minCheck=0.80

identifier swissProtAccession external=SwissProt
"SwissProt Accession"
     swissProt.displayId.acc
     swissProt.accToKeyword.acc 
     swissProt.accToTaxon.acc full
     swissProt.citation.acc
     swissProt.comment.acc
     swissProt.description.acc unique full
     swissProt.extDbRef.acc
     swissProt.feature.acc
     swissProt.gene.acc
     swissProt.geneLogic.acc
     swissProt.info.acc unique full
     swissProt.otherAcc.acc
     swissProt.protein.acc unique full
     $kgDb.kgProtAlias.alias  minCheck=0.2
     $kgDb.kgProtMap.qName minCheck=0.9
     $kgDb.kgXref.spID minCheck=0.9
     $gbd.pepMwAa.accession minCheck=0.9
     $gbd.pepPi.accession minCheck=0.9
     $ce.sangerCanonical.protein minCheck=0.8
     $ce.sangerGene.proteinID minCheck=0.8
     $ce.sangerLinks.protName minCheck=0.8
     $dm.bdgpCanonical.protein minCheck=0.9
     $dm.bdgpGene.proteinID minCheck=0.9
     $dm.bdgpSwissProt.swissProtId minCheck=0.9
     $dm.flyBaseSwissProt.swissProtId minCheck=0.9
     $sacCer.sgdGene.proteinID minCheck=0.9
     $sacCer.sgdCanonical.protein minCheck=0.9
     $sacCer.sgdToSwissProt.value minCheck=0.9
     go.goaPart.dbObjectId minCheck=0.99

identifier swissProtAuthor
"Author ID in SwissProt (just use inside UCSC Relationalized SwissProt)"
