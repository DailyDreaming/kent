     $gbd.dupSpMrna.proteinID
     $gbd.dupSpMrna.dupProteinID
     $knownDb.kgProtAlias.displayID
     $knownDb.kgXref.spDisplayID
     $knownDb.knownCanonical.protein
     $knownDb.knownGene.proteinID
     $gbd.spMrna.spID
     $dm.bdgpSwissProt.spSymbol
     $dm.flyBaseSwissProt.spSymbol
     go.goaPart.dbObjectSymbol

identifier swissProtAccession external=SwissProt
"SwissProt Accession"
     swissProt.displayId.acc
     swissProt.accToKeyword.acc 
     swissProt.accToTaxon.acc
     swissProt.citation.acc
     swissProt.comment.acc
     swissProt.description.acc
     swissProt.extDbRef.acc
     swissProt.feature.acc
     swissProt.gene.acc
     swissProt.geneLogic.acc
     swissProt.info.acc
     swissProt.otherAcc.acc
     swissProt.protein.acc
     $knownDb.kgProtAlias.alias 
     $knownDb.kgProtMap.qName
     $knownDb.kgXref.spID
     $gbd.pepMwAa.accession
     $gbd.pepPi.accession
     $ce.sangerCanonical.protein
     $ce.sangerGene.proteinID
     $ce.sangerLinks.protName
     $dm.bdgpCanonical.protein
     $dm.bdgpGene.proteinID
     $dm.bdgpSwissProt.swissProtId
     $dm.flyBaseSwissProt.swissProtId
     $sacCer.sgdGene.proteinID
     $sacCer.sgdCanonical.protein
     $sacCer.sgdToSwissProt.value
     go.goaPart.dbObjectId

