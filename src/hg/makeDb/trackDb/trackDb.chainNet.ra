# release alpha until individual fr2 tracks are obsolete
track chainNetFr2
release alpha
compositeTrack on
shortLabel Fugu Chain/Net
longLabel Fugu (Oct. 2004/fr2), Chain and Net Alignments
subGroup1 view Views chain=Chain net=Net
dragAndDrop subTracks
visibility hide
visibilityViewDefaults chain=pack net=dense
group compGeno
noInherit on
priority 159
color 0,0,0
altColor 255,255,0
type bed 3
settingsByView chain:spectrum=on
sortOrder view=+
matrix 16 91,-90,-25,-100,-90,100,-100,-25,-25,-100,100,-90,-100,-25,-90,91
matrixHeader A, C, G, T
otherDb fr2
chainMinScore 5000
chainLinearGap loose
spectrum on
html chainNet

    track chainFr2
    release alpha
    subTrack chainNetFr2
    subGroups view=chain
    shortLabel $o_Organism Chain
    longLabel $o_Organism ($o_date/$o_db) Chained Alignments
    type chain fr2

    track netFr2
    release alpha
    subTrack chainNetFr2
    subGroups view=net
    shortLabel $o_Organism Net
    longLabel $o_Organism ($o_date/$o_db) Alignment Net
    type netAlign fr2 chainFr2

