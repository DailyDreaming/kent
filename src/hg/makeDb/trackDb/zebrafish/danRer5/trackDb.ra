# Local declaration so that local gold.html is picked up.
track gold
shortLabel Assembly
longLabel Assembly from Fragments
group map
priority 10
visibility hide
color 150,100,30
altColor 230,170,40
type bed 3 +

# Local declaration so that local gap.html is picked up.
track gap
shortLabel Gap
longLabel Gap Locations
priority 11
group map
visibility dense
type bed 3 +

# Unsplit mRNA track for scaffold-based assembly.
track all_mrna
shortLabel $Organism mRNAs
longLabel $Organism mRNAs from GenBank
group rna
priority 54
visibility pack
spectrum on
type psl .
baseColorUseCds genbank
baseColorUseSequence genbank
baseColorDefault diffCodons
showDiffBasesAllScales .
indelDoubleInsert on
indelQueryInsert on
indelPolyA on

track vegaGene
shortLabel Vega Genes
longLabel Vega v27 Annotations
group genes
priority 37
visibility hide
chromosomes chr1,chr2,chr3,chr4,chr5,chr6,chr7,chr8,chr9,chr10,chr11,chr12,chr13,chr14,chr15,chr16,chr17,chr18,chr19,chr20,chr21,chr22,chr23,chr24,chr25,Zv7_scaffold2491,Zv7_scaffold2498,Zv7_scaffold2509,Zv7_scaffold2516,Zv7_scaffold2523,Zv7_scaffold2533,Zv7_scaffold2537,Zv7_scaffold2551,Zv7_scaffold2560,Zv7_scaffold2633,Zv7_scaffold2650
color 12,12,120
type genePred
dataVersion VEGA Release 27
url http://vega.sanger.ac.uk/Danio_rerio/geneview?transcript=$$

track vegaPseudoGene
shortLabel Vega Pseudogenes
longLabel Vega v27 Annotated Pseudogenes 
group genes
priority 37.1
visibility hide
chromosomes chr2.chr3,chr4,chr5,chr9,chr12,chr13,chr18,chr19,chr20,chr24,Zv7_scaffold2509 
color 12,12,120
type genePred
dataVersion VEGA Release 27
url http://vega.sanger.ac.uk/Danio_rerio/geneview?transcript=$$

# Unsplit EST track for scaffold-based assembly.
track all_est
shortLabel $Organism ESTs
longLabel $Organism ESTs Including Unspliced
group rna
priority 57
visibility hide
spectrum on
intronGap 30
type psl est
baseColorUseSequence genbank
indelDoubleInsert on
indelQueryInsert on
maxItems 300

track affyZebrafish
compositeTrack on
shortLabel Affy Zebrafish
longLabel Alignments of Affymetrix Sequences from Zebrafish Chip
group regulation
priority 89
visibility hide
type psl .

    track affyZebrafishConsensus
    subTrack affyZebrafish
    shortLabel Affy Zfish Cons
    longLabel Alignments of Affymetrix Consensus Sequences from Zebrafish Chip
    priority 1
    
    track affyZebrafishTarget
    subTrack affyZebrafish
    shortLabel Affy Zfish Targ
    longLabel Alignments of Affymetrix Target Sequences from Zebrafish Chip
    priority 1

