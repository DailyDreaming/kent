#Some mosquito-specific stuff.

track mrna override
spectrum on

track intronEst
#replaces record intronEst in parent dir missing/extra maxItems
shortLabel Spliced ESTs
longLabel $Organism ESTs That Have Been Spliced
group rna
priority 56
visibility dense
spectrum on
intronGap 30
type psl est
baseColorUseSequence genbank
showDiffBasesAllScales .
indelDoubleInsert on
indelQueryInsert on

track est
#replaces record est in parent dir missing/extra maxItems
shortLabel $Organism ESTs
longLabel $Organism ESTs Including Unspliced
group rna
priority 57
visibility hide
spectrum on
intronGap 30
type psl est
baseColorUseSequence genbank
indelDoubleInsert on
indelQueryInsert on

track blastDm1FB override
shortLabel D. mel. Proteins (dm1)
longLabel $o_Organism ($o_date/$o_db) Proteins
visibility pack
otherDb dm1

track blastDm2FB override
shortLabel D. mel. Proteins
longLabel $o_Organism ($o_date/$o_db) Proteins
otherDb dm2

track rmsk
#replaces record rmsk in parent dir missing/extra maxWindowToDraw
shortLabel RepeatMasker
longLabel Repeating Elements by RepeatMasker
group varRep
priority 149.1
visibility dense
spectrum on
type rmsk
canPack off

#Overrides from visibility.ra

track ensGene override
visibility dense

