include trackDb.chainNet.ra

track contigOrient
shortLabel Contig Orient
longLabel Contig Orientation
priority 15
group map
visibility hide
type bed 6 .
useScore 1
chromosomes chr1,chr10,chr11,chr12,chr13,chr14,chr15,chr16,chr17,chr18,chr19,chr2,chr20,chr21,chr22,chr23,chr24,chr25,chr26,chr27,chr28,chr29,chr3,chr4,chr5,chr6,chr7,chr8,chr9,chrX


track all_mrna
shortLabel $Organism mRNAs
longLabel $Organism mRNAs from GenBank 
group rna
priority 54
visibility pack
spectrum on
type psl .
baseColorUseCds genbank
baseColorUseSequence genbank
baseColorDefault diffCodons
showDiffBasesAllScales .
indelDoubleInsert on
indelQueryInsert on
indelPolyA on

track all_est
shortLabel $Organism ESTs
longLabel $Organism ESTs Including Unspliced
group rna
priority 57
visibility hide
spectrum on
intronGap 30
type psl est
baseColorUseSequence genbank
indelDoubleInsert on
indelQueryInsert on

track blastHg17KG override
shortLabel Human Proteins
longLabel Human(hg17) proteins mapped by chained tBLASTn
priority 30
pred hg17.blastKGPep02
blastRef hg17.blastKGRef02

track sgpGene override
longLabel SGP Gene Predictions Using Cow/Mouse Homology

track chainHg18
#replaces record chainHg18 in parent dir missing/extra matrix,matrixHeader
release beta
shortLabel $o_Organism Chain 
longLabel $o_Organism ($o_date/$o_db) Chained Alignments
group compGeno
priority 157
visibility hide
color 100,50,0
altColor 255,240,200
spectrum on
type chain hg18
otherDb hg18

track netHg18 override
priority 157.5
visibility hide

searchTable gold
searchMethod exact
query select chrom,chromStart,chromEnd,frag from %s where frag like '%s'
shortCircuit 1
termRegex (Contig|Reptig|gnl)[0-9]+
searchPriority 8

searchName blastHg17KGSprot
searchTable blastHg17KG
searchMethod prefix
searchType psl
xrefTable hg17.blastKGRef02
xrefQuery select acc,extra1 from %s where extra1 like '%s%%'
searchPriority 25

searchName blastHg17KGGene
searchTable blastHg17KG
searchMethod prefix
searchType psl
xrefTable hg17.blastKGRef02
xrefQuery select acc,geneId from %s where geneId like '%s%%'
searchPriority 25

searchTable blastHg17KG
searchMethod prefix
searchType psl

