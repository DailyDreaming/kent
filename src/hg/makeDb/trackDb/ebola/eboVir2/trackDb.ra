# Local declaration so that local gold.html is picked up.
track gold override
visibility pack
html gold

# Local declaration so that local gap.html is picked up.
track gap override
visibility hide
html gap

track chainSelf
shortLabel 47 Strain Chain
longLabel 47 strains Chained Alignments
group compGeno
chainColor Normalized Score
visibility hide
matrixHeader A, C, G, T
matrix 16 91,-90,-25,-100,-90,100,-100,-25,-25,-100,100,-90,-100,-25,-90,91
type chain eboVir2
otherDb eboVir2
chainNormScoreAvailable yes
color 100,50,0
altColor 255,240,200
spectrum on

#track genePredictions
#shortLabel genePredictions
#longLabel Gene predictions from the five annotated NCBI genomes
#group rna
#visibility pack
#spectrum on
#color 12,12,120
#type psl protein

track ncbiGene
shortLabel NCBI Genes
longLabel NCBI Genes from http://www.ncbi.nlm.nih.gov/nuccore/674810552
group genes
visibility pack
color 12,12,120
type genePred 
baseColorUseCds given
baseColorDefault genomicCodons

track gire2014
shortLabel 2014 Variants
longLabel Variants from 81 samples from 2014 outbreak
group varRep
type vcfTabix
visibility hide

track gireZebov
shortLabel Zaire Variants
longLabel Variants from 102 ebola sequences from West Africa 2014 outbreak
group varRep
type vcfTabix
visibility hide

track gire2014Missense
shortLabel 2014 Missense
longLabel Missense variants from 81 samples from 2014 outbreak
group varRep
type vcfTabix
visibility hide

track gireZebovMissense
shortLabel Zaire Missense
longLabel Missense variants from 101 ebola sequences from West Africa 2014 outbreak
group varRep
type vcfTabix
visibility pack

track iedb
shortLabel IEDB T-Cell Epitopes
longLabel IEDB T-Cell Epitopes
group regulation
type bigBed 12 .
visibility hide
urls pubMedID=http://www.ncbi.nlm.nih.gov/pubmed/$$

track iedbBcell
shortLabel IEDB B-Cell Epitopes
longLabel IEDB B-Cell Epitopes
group regulation
type bigBed 12 .
visibility dense
urls pubMedID=http://www.ncbi.nlm.nih.gov/pubmed/$$ bCellID=http://www.iedb.org/assayId/$$ referenceID=http://www.iedb.org/refId/$$ epitopeID=http://www.iedb.org/epId/$$ epitopeSourceMoleculeAccession=http://www.ncbi.nlm.nih.gov/protein/$$

include trackDb.49way.ra
include trackDb.49wayStrainNames.ra
