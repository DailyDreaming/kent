###### Harvard University large intergenic non-coding RNAs (lincRNAs)
track xlincRNAs
shortLabel linc RNAs
longLabel Large intergenic noncoding RNAs
group x
#priority 45.25
visibility hide
type expRatio
expScale 4.0
expStep 0.5
expDrawExons on
expTable lincRNAsExpAll
groupings lincRNAsExpGroups
html lincRNAs

track xlincRNAsTranscripts
shortLabel lincRNAs Transcripts
longLabel lincRNAsTranscripts
group x
#priority 45.5
visibility hide
color 100,50,0
altColor 175,150,128
type genePred


track lincRNAs
superTrack on
shortLabel lincRNAsAll
longLabel lincRNAsAll
group expression
#visibility dense 
html lincRNAs

    track lincRNAsIsoforms
    superTrack lincRNAs pack
    shortLabel lincRNAsTranscriptIsoforms
    longLabel lincRNAs Transcript Isoforms
    #shadesOfBrown
    color 100,50,0
    altColor 175,150,128
    type genePred
    noInherit on


    track lincRNAsAllCellTypeTopView
    superTrack lincRNAs full
    compositeTrack on
    shortLabel lincRNAsAllCellTypeView
    longLabel lincRNAs All Cell Type View
    subGroup1 view Views lincRNAsRefseqExp=RefSeq_Expression_Ratio
#    subGroup2 tissueType Tissue_Type brain=Brain heart=Heart
        subGroup2 tissueType Tissue_Type adipose=Adipose adrenal=Adrenal brain=Brain breast=Breast colon=Colon heart=Heart kidney=Kidney liver=Liver lung=Lung lymphnode=LymphNode ovary=Ovary prostate=Prostate skeletalmuscle=SkeletalMuscle whitebloodcell=WhiteBloodCell testes=Testes thyroid=Thyroid testes_r=Testes_R brain_r=Brain_R placenta_r=Placenta_R foreskin_r=Foreskin_R hlf_r2=hLF_r2 hlf_r1=hLF_r1
    sortOrder view=+ tissueType=+
    dimensions dimensionY=tissueType
    priority 0
    dragAndDrop subTracks
    configurable on
    noInherit on
    type bed 5 +

    track lincRNAsAllCellType
    shortLabel lincRNAsAllCellType
    longLabel lincRNAs All Cell Types
    view lincRNAsRefseqExp
    visibility dense
    parent lincRNAsAllCellTypeTopView
    # shadesOfSea
    color 0, 60, 120
    useScore 1
    extraFields rawScore=[f]Raw_Score \
                log2RawScore=[f]log2(Raw_Score)

#edge

	track lincRNAsCTAdipose 
	parent lincRNAsAllCellType on
	shortLabel Adipose
	longLabel lincRNAs from adipose 
	subGroups view=lincRNAsRefseqExp tissueType=adipose

