track previousVersionsChainNet
compositeTrack on
shortLabel Aged Out Chain/Net
longLabel Previous genome versions, Chain and Net Alignments
subGroup1 view Views chain=Chains net=Nets
subGroup2 species Species aRat=Rat bChimp=Chimp
subGroup3 clade Clade aRodent=Rodent bHom=Hominidae
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 500
color 0,0,0
altColor 255,255,0
type bed 3
chainLinearGap medium
chainMinScore 5000
dimensions dimensionX=clade dimensionY=species 
sortOrder species=+ view=+ clade=+
configurable on
html mm10previousVersionsChainNet

    track previousVersionsChainNetViewchain
    shortLabel Chains
    view chain
    visibility pack
    subTrack previousVersionsChainNet
    spectrum on

        track chainRn4
        subTrack previousVersionsChainNetViewchain on
        subGroups view=chain species=aRat clade=aRodent
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain rn4
        otherDb rn4

        track chainPanTro3
        subTrack previousVersionsChainNetViewchain off
        subGroups view=chain species=bChimp clade=bHom
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain panTro3
        otherDb panTro3

    track previousVersionsChainNetViewnet
    shortLabel Nets
    view net
    visibility dense
    subTrack previousVersionsChainNet

        track netRn4
        subTrack previousVersionsChainNetViewnet on 
        subGroups view=net species=aRat clade=aRodent
        shortLabel $o_Organism Net
        longLabel $o_Organism ($o_date) Alignment Net
        type netAlign rn4 chainRn4
        otherDb rn4

        track netPanTro3
        subTrack previousVersionsChainNetViewnet off
        subGroups view=net species=bChimp clade=bHom
        shortLabel $o_Organism Net
        longLabel $o_Organism ($o_date) Alignment Net
        type netAlign panTro3 chainPanTro3
        otherDb panTro3
