priority 260
visibility hide
color 100,50,0
altColor 255,240,200
spectrum on
matrix 16 56,-109,-45,-137,-109,100,-103,-45,-45,-103,100,-109,-137,-45,-109,56
matrixHeader A, C, G, T
type chain rn4
otherDb rn4

track netRn4Test
