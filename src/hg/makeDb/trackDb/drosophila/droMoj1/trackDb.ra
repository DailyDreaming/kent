track chainDm1 override
shortLabel D. mel. Chain ($o_db)

track netDm1
#replaces record netDm1 in parent dir missing/extra release
shortLabel D. mel. Net ($o_db)
longLabel $o_Organism ($o_date/$o_db) Alignment Net
group compGeno
priority 134
visibility dense
spectrum on
type netAlign dm1 chainDm1
otherDb dm1

track chainDm2 override
priority 133

track netDm2 override
priority 134
visibility dense

track all_est
shortLabel $Organism ESTs
longLabel $Organism ESTs Including Unspliced
group rna
priority 57
visibility hide
spectrum on
type psl est
baseColorUseSequence genbank
indelDoubleInsert on
indelQueryInsert on

track all_mrna
shortLabel D. moj. mRNAs
longLabel $Organism mRNAs from GenBank 
group rna
priority 54
visibility dense
spectrum on
type psl mrna
baseColorUseCds genbank
baseColorUseSequence genbank
baseColorDefault diffCodons
showDiffBasesAllScales .
indelDoubleInsert on
indelQueryInsert on
indelPolyA on

track bdgpLiftGene
shortLabel D.mel. BDGP Genes
longLabel $o_Organism ($o_date/$o_db) BDGP Genes Mapped to $Organism
group genes
priority 45.5
visibility pack
color 0,100,180
type genePred bdgpLiftGenePep
otherDb dm1

track blastDm1FB override
shortLabel D.mel. Proteins (dm1)
longLabel $o_Organism ($o_date/$o_db) Proteins
otherDb dm1

track blastDm2FB override
shortLabel D. mel. Proteins
longLabel $o_Organism ($o_date/$o_db) Proteins
visibility pack
otherDb dm2


searchTable bdgpLiftGene
searchMethod exact
searchType genePred
termRegex CG[0-9]+-R.
searchPriority 15

searchName bdgpLiftGenePrefix
searchTable bdgpLiftGene
searchMethod exact
termRegex CG[0-9]+
dontCheck CG[0-9]+-R[A-Z]
query select chrom,txStart,txEnd,name from %s where name like '%s-R_'
searchPriority 16

searchTable genscan
searchType genePred
termRegex contig_[0-9]+\.[0-9]+
searchPriority 50

