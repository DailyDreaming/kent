#
# generate download files on hgdownload
#

SHELL=/bin/sh
MAILTO=genbank@cse.ucsc.edu
PATH=/usr/local/bin:/usr/bin:/bin

# copy new download files from genbank server
# poll hourly
30 * * * * rsync -e '/usr/bin/rsh' --whole-file -rl --include='*.fa.gz' --include='*.maf.gz' --exclude='*.tmp*' genbank-10:/cluster/data/genbank/data/ftp/ /mirrordata/apache/htdocs/goldenPath
