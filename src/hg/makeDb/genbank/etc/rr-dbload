#!/bin/bash -e
#
# rr-dbload [-inclEsts]
#
# dbload for rr servers.
opts="$*"

# databases to update
databases="rn3 hg16"

# gbRoot on hgnfs1
gbRoot=/genbank

# workdir is a local dir
workdir=/var/tmp/genbank

# wait for copy to hgwnfs1
copyTime=var/copy/copy.time

exec $gbRoot/etc/dbload $opts $gbRoot $databases $copyTime $workdir
