# Append the below lines to the standard RR hg.conf and set cart.password to
# create the MGC browser hg.conf


##
# MGC specific settings
##
mgc.server=yes
db.trackDb=trackDbMgc,trackDbCcds,trackDb
db.grp=grpMgc,grpCcds,grp
browser.background=../images/floretMgc.jpg

# MGC stores cart locally due to different track configs
cart.db=cartDb
cart.host=localhost
cart.user=hgcentuser
cart.password=<SAME AS DB PASS>

