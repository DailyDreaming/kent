include ../../inc/common.mk
HG_WARN_ERR = -DJK_WARN -Wall -Werror

L = -lm 
MYLIBDIR = ../../lib/$(MACHTYPE)
MYLIBS =  ../lib/$(MACHTYPE)/paralib.a $(MYLIBDIR)/jkhgap.a $(MYLIBDIR)/jkweb.a

O = broadNode.o

broadNode: $O $(MYLIBS)
	gcc -ggdb -o ../bin/broadNode $O $(MYLIBS) $L
	strip ../bin/broadNode
